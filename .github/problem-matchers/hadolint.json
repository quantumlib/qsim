{
  "problemMatcher": [
    {
      "owner": "hadolint",
      "pattern": [
        {
          "regexp": "^(.*?):(\\d+) (DL\\d+|SC\\d+) (warning|error|info|style): (.*)$",
          "file": 1,
          "line": 2,
          "code": 3,
          "severity": 4,
          "message": 5
        }
      ]
    }
  ]
}
